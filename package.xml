<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<package format="1" xmlns="https://wiki.freecad.org/Package_Metadata">
  <name>Format Migrator</name>
  <description>A tool for upconverting and downconverting FreeCAD files between versions</description>
  <version>0.1</version>
  <maintainer email="chennes@freecad.org">Chris Hennes</maintainer>
  <license file="LICENSE">LGPL-2.1-or-later</license>
  <url type="repository" branch="master">https://github.com/FreeCAD/FCStdMigrator</url>
  <url type="documentation">https://wiki.freecad.org/FCStdMigrator</url>

  <content>
    <workbench>
      <name>Format Migrator</name>
      <classname>FCStdMigrator</classname>
      <subdirectory>./</subdirectory>
      <icon>freecad/fcstdmigrator/resources/icons/FCStdMigrator.svg</icon>
      <freecadmin>0.20.0</freecadmin>
      <depend type="python">defusedxml</depend>
      <depend type="python">packaging</depend>
      <tag>format</tag>
      <tag>utility</tag>
    </workbench>
  </content>

</package>
